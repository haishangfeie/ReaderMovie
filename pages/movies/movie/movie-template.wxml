<import src="/pages/movies/stars/stars-template.wxml" />
<template name="movieTemplate">
    <view class="movie" catchtap="onMovieTap" data-movie-id = "{{movieId}}">
        <image class="movie-cover" src="{{coverageUrl}}"  />
        <text class="movie-name">{{title}}</text>
        <template is="starsTemplate" data="{{average,stars}}"></template>
    </view>
</template>